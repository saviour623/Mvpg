 #!/usr/bin/calc


define log2(a) {
       return log(a) / log(2)
}
define convert(a) {
       local p10, p2, edp, dpexp

       p10 = trunc(log(a));
       p2  = trunc(log2(a));

       edp = p10*ln(10) - p2*ln(2);

       dpexp = exp(edp);
       printf("%.30f\n", a / ((2**p2) * dpexp));
}

define main() {
       local a =  2342242344534534545234344534.;

       convert(a);
}

main();